<?xml version='1.0' encoding='UTF-8'?>
<configuration debug="true">

	<consolePlugin />

	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">

		<encoder>
			<pattern>%d %5p | %-20t | %-25logger{25} | %m %n</pattern>
		</encoder>

	</appender>

	<root>
		<level value="DEBUG" />
		<appender-ref ref="CONSOLE" />
	</root>

</configuration>
